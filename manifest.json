{
  "name": "Lucky Strike",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Redirects omnibox queries to Google's I'm Feeling Lucky",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Go to search results"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "omnibox": {
    "keyword": "l"
  }
}